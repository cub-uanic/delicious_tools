{
  "content_scripts": [
    {
      "js": [
        "delicious_content_script.js"
      ], 
      "matches": [
        "http://*/*"
      ]
    }
  ], 
  "name": "Del.icio.us Tools",
  "version": "1.0",
  "description": "Del.icio.us toolbar for Chrome",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
      "default_title": "Delicious",
      "default_icon": "delicious.png",
      "popup": { "path": "popup.html" }
  }  
}